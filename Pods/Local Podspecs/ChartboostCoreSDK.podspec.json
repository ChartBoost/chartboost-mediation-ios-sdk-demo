{
  "name": "ChartboostCoreSDK",
  "version": "1.0.0",
  "license": {
    "type": "MIT",
    "file": "LICENSE.md"
  },
  "homepage": "https://github.com/ChartBoost/chartboost-core-ios-sdk",
  "authors": {
    "Chartboost": "https://www.chartboost.com/"
  },
  "summary": "Chartboost Core iOS SDK.",
  "description": "SDK that provides core functionalities to publishers and other modules.",
  "documentation_url": "https://reference.chartboost.com/core/ios/1.0.0",
  "module_name": "ChartboostCoreSDK",
  "source": {
    "git": "https://github.com/ChartBoost/chartboost-core-ios-sdk.git",
    "tag": "1.0.0"
  },
  "source_files": "Source/**/*.{swift}",
  "resource_bundles": {
    "ChartboostCoreSDK": [
      "PrivacyInfo.xcprivacy"
    ]
  },
  "static_framework": true,
  "swift_versions": "5.0",
  "platforms": {
    "ios": "13.0"
  },
  "ios": {
    "frameworks": [
      "AdSupport",
      "AppTrackingTransparency",
      "AVFoundation",
      "CoreTelephony",
      "Foundation",
      "SystemConfiguration",
      "UIKit",
      "WebKit"
    ]
  },
  "prepare_command": "mkdir -p Tests/TestInfrastructure/AutoGenerated && touch Tests/TestInfrastructure/AutoGenerated/mockable.generated.swift",
  "testspecs": [
    {
      "name": "Tests",
      "test_type": "unit",
      "source_files": "Tests/**/*.{swift,m}",
      "platforms": {
        "ios": "14.0"
      },
      "dependencies": {
        "Sourcery": [
          "~> 2.0"
        ]
      },
      "script_phases": {
        "name": "Auto-generate mocks",
        "script": "$PODS_ROOT/Sourcery/bin/sourcery --config /Users/daniel-barros/helium-ios/Core",
        "execution_position": "before_compile"
      }
    }
  ],
  "swift_version": "5.0"
}
