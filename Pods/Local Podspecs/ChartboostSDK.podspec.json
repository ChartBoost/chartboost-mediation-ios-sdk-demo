{
  "name": "ChartboostSDK",
  "version": "9.8.0",
  "license": {
    "type": "Commercial",
    "text": "LICENSE Â© 2011-2024 Chartboost. All rights reserved. LICENSE"
  },
  "summary": "Chartboost iOS SDK.",
  "homepage": "https://chartboost.com/",
  "authors": {
    "Chartboost": "https://chartboost.com/"
  },
  "description": "A technology platform that helps mobile game developers find new users and monetize their games.",
  "documentation_url": "https://docs.chartboost.com/",
  "readme": "https://chartboost.s3.amazonaws.com/sdk/README.md",
  "module_name": "ChartboostSDK",
  "platforms": {
    "ios": "12.0"
  },
  "frameworks": [
    "AdSupport",
    "CoreGraphics",
    "StoreKit",
    "Foundation",
    "UIKit",
    "WebKit",
    "AVFoundation",
    "CoreMedia",
    "SystemConfiguration",
    "CoreFoundation",
    "SafariServices",
    "CoreTelephony",
    "AppTrackingTransparency"
  ],
  "requires_arc": true,
  "pod_target_xcconfig": {
    "OTHER_LDFLAGS": "-lObjC"
  },
  "swift_versions": "5.0",
  "source": {
    "http": ""
  },
  "source_files": [
    "Source/**/*.{swift,h,m}",
    "OMSDK/OMSDK-Static_Chartboost.xcframework/ios-arm64/Headers/*.h"
  ],
  "public_header_files": [
    "Source/Ads/Banner/CHBBanner.h",
    "Source/Ads/Models/CHBAdEvent.h",
    "Source/Ads/Models/CHBMediation.h",
    "Source/Ads/CHBAdDelegate.h",
    "Source/Privacy/CHBDataUseConsent.h",
    "Source/Ads/Rewarded/CHBRewarded.h",
    "Source/Supporting Files/ChartboostSDK.h",
    "Source/Ads/CHBAd.h",
    "Source/Chartboost/Chartboost.h",
    "Source/Ads/Interstitial/CHBInterstitial.h"
  ],
  "private_header_files": [
    "Source/External/OMSDKImports.h",
    "OMSDK/OMSDK-Static_Chartboost.xcframework/ios-arm64/Headers/*.h"
  ],
  "prefix_header_file": "Source/Supporting Files/Chartboost-Prefix.pch",
  "module_map": "Source/Supporting Files/ChartboostSDK.modulemap",
  "vendored_frameworks": "OMSDK/OMSDK-Static_Chartboost.xcframework",
  "resource_bundles": {
    "ChartboostSDK": [
      "PrivacyInfo.xcprivacy"
    ],
    "ChartboostSDKResources": [
      "Resources/info.png",
      "Resources/omsdk-v1.js"
    ]
  },
  "static_framework": true,
  "prepare_command": "mkdir -p Tests/Common/TestInfrastructure/AutoGenerated && touch Tests/Common/TestInfrastructure/AutoGenerated/mockable.generated.swift",
  "testspecs": [
    {
      "name": "UnitTests",
      "test_type": "unit",
      "source_files": [
        "Tests/Common/**/*.{swift,h,m}",
        "Tests/UnitTests/**/*.{swift,h,m}",
        "Tests/Tests-Bridging-Header.h"
      ],
      "resources": "Tests/Resources/**/*",
      "pod_target_xcconfig": {
        "SWIFT_OBJC_BRIDGING_HEADER": "/Users/daniel-barros/helium-ios/Monetization/Tests/Tests-Bridging-Header.h"
      },
      "dependencies": {
        "Sourcery": [
          "~> 2.0"
        ]
      },
      "script_phases": {
        "name": "Auto-generate mocks",
        "script": "$PODS_ROOT/Sourcery/bin/sourcery --config /Users/daniel-barros/helium-ios/Monetization",
        "execution_position": "before_compile"
      }
    },
    {
      "name": "IntegrationTests",
      "test_type": "unit",
      "source_files": [
        "Tests/Common/**/*.{swift,h,m}",
        "Tests/IntegrationTests/**/*.{swift,h,m}",
        "Tests/Tests-Bridging-Header.h"
      ],
      "resources": "Tests/Resources/**/*",
      "pod_target_xcconfig": {
        "SWIFT_OBJC_BRIDGING_HEADER": "/Users/daniel-barros/helium-ios/Monetization/Tests/Tests-Bridging-Header.h"
      },
      "requires_app_host": true,
      "app_host_name": "ChartboostSDK/IntegrationTestsHostApp",
      "dependencies": {
        "ChartboostSDK/IntegrationTestsHostApp": [

        ],
        "Sourcery": [
          "~> 2.0"
        ]
      },
      "script_phases": {
        "name": "Auto-generate mocks",
        "script": "$PODS_ROOT/Sourcery/bin/sourcery --config /Users/daniel-barros/helium-ios/Monetization",
        "execution_position": "before_compile"
      }
    }
  ],
  "appspecs": [
    {
      "name": "IntegrationTestsHostApp",
      "source_files": "Tests/TestsHostApp.swift"
    }
  ],
  "swift_version": "5.0"
}
